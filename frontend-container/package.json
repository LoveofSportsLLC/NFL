  "main": "server.js",
  "scripts": {
    "uat:full": "docker compose -f ./docker-compose.uat.yml down && docker compose -f ./docker-compose.uat.yml build --no-cache && docker compose -f ./docker-compose.uat.yml up -d && docker compose -f ./docker-compose.uat.yml logs -f",
    "prod:full": "docker compose -f ./docker-compose.prod.yml down && docker compose -f ./docker-compose.prod.yml build --no-cache && docker compose -f ./docker-compose.prod.yml up -d && docker compose -f ./docker-compose.prod.yml logs -f",
    "setReleaseVersion": "node ./scripts/setReleaseVersion.js",
    "dev": "NODE_DEBUG=http2,express NODE_ENV=development node --trace-warnings server.js",
    "start:dev": "npm run build:dev  && npm run dev",
    "clean": "echo 'Cleaning dist directory' && rm -rf dist",
    "build:client:prod": "echo 'Building client (production)' && BUILD_TARGET=client NODE_ENV=production NODE_OPTIONS=--max-old-space-size=12288 vite build --mode production --ssrManifest --logLevel warn",
    "build:server:prod": "echo 'Building server (production)' && BUILD_TARGET=ssr NODE_ENV=production NODE_OPTIONS=--max-old-space-size=12288 vite build --mode production --ssr --logLevel warn",
    "build:dev": "DOCKER_ENV=false vite build --mode development",
    "build:prod": "npm run build:client:prod && npm run build:server:prod",
    "start:prod": "npm run build:prod && NODE_DEBUG=http2,express NODE_ENV=production node --trace-warnings server.js",
    "check-circular-deps": "madge --circular --extensions js,jsx src",
    "webhint": "xvfb-run -a hint http://localhost:3000",
    "uatintercept": "telepresence quit && telepresence connect && telepresence intercept feuat --namespace uat --service feuat --port 3000:fehttp"
  },
