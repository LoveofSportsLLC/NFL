{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker: Attach to Node with Kubernetes",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "address": "localhost",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/app",
      "preLaunchTask": "Start Docker Compose Debug",
      "restart": true,
      "sourceMaps": false,
      "outFiles": [],
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
